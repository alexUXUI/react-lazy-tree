{
    "extends": "airbnb",
    "rules": {
        "indent": [1, 4],
        "comma-dangle": 0,
        "react/jsx-indent": [1, 4],
        "react/jsx-filename-extension": 0,
        "react/jsx-first-prop-new-line": 0,
        "react/jsx-closing-bracket-location": 0,
        "class-methods-use-this": 0,
        "no-param-reassign": 0
    }
}
